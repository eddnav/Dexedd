            <enum name="bug" value="7" />
            <enum name="ghost" value="8" />
            <enum name="steel" value="9" />
            <enum name="fire" value="10" />
            <enum name="water" value="11" />
            <enum name="grass" value="12" />
            <enum name="electric" value="13" />
            <enum name="psychic" value="14" />
            <enum name="ice" value="15" />
            <enum name="dragon" value="16" />
            <enum name="dark" value="17" />
            <enum name="fairy" value="18" />
